{
  "version": "1.0.0-eta-01",
  "authors": [ "Stephen Cleary" ],
  "description": "Automatic implementation of change detection for MVVM properties.",
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "mvvm", "inotifypropertychanged", "calculated", "properties" ],
    "projectUrl": "https://github.com/StephenCleary/Mvvm.CalculatedProperties",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/Mvvm.CalculatedProperties/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/Mvvm.CalculatedProperties.git"
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Linq": "4.1.0",
        "System.Linq.Expressions": "4.1.0",
        "System.ObjectModel": "4.0.12",
        "System.Reflection": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "frameworkAssemblies": {
        "System.Collections": { "type": "build" },
        "System.Diagnostics.Debug": { "type": "build" },
        "System.Linq": { "type": "build" },
        "System.Linq.Expressions": { "type": "build" },
        "System.ObjectModel": { "type": "build" },
        "System.Reflection": { "type": "build" },
        "System.Runtime": { "type": "build" },
        "System.Runtime.Extensions": { "type": "build" }
      }
    }
  },

  "dependencies": {
    "Nito.Mvvm.Core": "1.0.0-eta-01"
  }
}
